
source 'https://github.com/CocoaPods/Specs.git'
use_frameworks!

def alamofire
  pod 'Alamofire', '>= 4.8.2'
end

target 'ShowDictionary' do
  platform :ios, '13.0'

  # Pods for ShowDictionary
  alamofire
end

target 'ShowDictionaryTVOS' do
  platform :tvos, '13.0'
 
  # Pods for ShowDictionaryTVOS
  alamofire
end

target 'ShowDictionaryWatchOS Extension' do
  platform :watchos, '6.0'

  # Pods for ShowDictionaryWatchOS Extension
  alamofire
end
